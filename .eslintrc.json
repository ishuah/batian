{
    "extends": [
        "airbnb",
        "plugin:@typescript-eslint/recommended"
    ],
    "settings": {
        "import/resolver": {
        "typescript": {}
        }
    },
    "rules":{
        "react/jsx-filename-extension": [ "warn", {"extensions": [".tsx"]} ],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "ts": "never",
                "tsx": "never"
            }
        ]
    }
        
}