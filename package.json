{
  "name": "batian-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://app.batian.io",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "axios": "^0.28.0",
    "d3": "^7.4.3",
    "gh-pages": "^5.0.0",
    "grommet": "^2.21.0",
    "grommet-icons": "^4.7.0",
    "papaparse": "^5.3.2",
    "polished": "^4.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "recoil": "^0.7.6",
    "styled-components": "^5.3.3",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!d3)/\"",
    "test:coverage": "react-scripts test --transformIgnorePatterns \"node_modules/(?!d3)/\" --coverage --watchAll=false",
    "lint": "npx eslint src/*",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --cname app.batian.io"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "sass": "^1.50.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts"
    ]
  }
}
